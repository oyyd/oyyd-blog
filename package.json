{
  "name": "blog",
  "version": "0.1.0",
  "description": "oyyd blog",
  "main": "index.html",
  "scripts": {
    "release": "babel tasks --out-dir lib/tasks && gulp --cwd . --gulpfile lib/tasks/gulpfile.js release",
    "watch": "babel tasks --out-dir lib/tasks && gulp --cwd . --gulpfile lib/tasks/gulpfile.js watch",
    "start:single": "node lib/server/app.js dev",
    "start:server": "node lib/server/app.js",
    "start": "pm2 start process.json",
    "start:dev": "pm2 start process.dev.json",

    "init": "babel src --out-dir lib",
    "watch:post": "gulp --gulpfile lib/tasks/gulpfile.js watch-post",
    "build:list": "gulp --gulpfile lib/tasks/gulpfile.js gen-list",
    "build:lib": "babel src --out-dir lib",
    "build:sitemap": "gulp --gulpfile lib/tasks/gulpfile.js gen-sitemap"
  },
  "author": "oyyd",
  "license": "MIT",
  "devDependencies": {
